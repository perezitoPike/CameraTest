  <script>
    var contentImages_UP = document.getElementById("contentImages");
    var contentImages_Down = document.getElementById("contentImages1");
    var currentImages = ("<%= images%>").split(',');
    console.log(currentImages);
    contentImages_UP.innerHTML += GetDivCreated(currentImages);
    contentImages_Down.innerHTML += GetDivCreated(currentImages.reverse());

    function GetDivCreated(currentImages) {
      var direction = "";
      currentImages.forEach(element => {
        direction += "<div class='swiper-slide swiper-auto-full ver2'> <img src='" + element + "' alt='' /></div>";
        // console.log(direction);
      });
      return direction;
    }
  </script>
